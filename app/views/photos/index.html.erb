<div class="center">
<h3><%= @user.first_name%>'s Photos</h3>
<% if current_user?(@user) %>
<p><%= link_to "Add New Photo", new_user_photo_path(@current_user), class: "waves-effect waves-light btn" %></p>
<% end %>
</div>
<div class="row">
<% @photos.each do |p| %>
  <% if p.user %>
   <div class="col s4">
    <div class="card">
      <div class="card-image">
      <%= link_to( image_tag(p.image), user_photo_path( @user, p) ) %>
          <span class="card-title"><%= p.title %></span>
            </div>
          </div>
        </div>
      <% end %>
    <% end %>
  </div>
