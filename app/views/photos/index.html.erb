<h1><%= @user.first_name%>'s Photos</h1>

<%= link_to "Add New Photo", new_user_photo_path(@current_user) %>

<% @photos.each do |p| %>
  <div class="row">
     <div ="col s4">
     <p>Photo Owner: <%= p.user.first_name %></p>
     <p>Photo Name: <%=p.title %></p>
     <%= link_to( image_tag(p.image), user_photo_path(@user, p) ) %>
  </div>
<% end %>
